<div class="col-md-4 col-md-offset-2 form-card">
	<div>
		<p>Profile Detail</p>
	</div>
	<div>
		<center>
			<table class="table table-hover text-center">
				<tr>
					<td>Name </td>
					<td>
						<%= name %>
					</td>
				</tr>
				<tr>
					<td>Email </td>
					<td>
						<%= email %>
					</td>
				</tr>
				<tr>
					<td>Date of Birth </td>
					<td>
						<%= dob %>
					</td>
				</tr>
				<tr class="alert alert-danger">
					<td>Birthday</td>
					<td>
						<%= birthdayMessage %>
					</td>
				</tr>
			</table>
		</center>

	</div>


</div>